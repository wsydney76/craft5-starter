{# @var field \craft\base\Field #}
{% set fields = craft.app.fields.allFields %}

<table>
    {% for field in fields %}

        <tr>
            <td>{{ loop.index }}</td>
            <td>{{ field.id }}</td>
            <td>{{ field.name }}</td>
            <td>{{ field.handle }}</td>
            <td>{{ className(field) }}</td>
            <td>
                {% if not (craft.app.fields.findFieldUsages(field)) %}
                    <span style="color:red;">No usages</span>
                {% endif %}
            </td>
            <td>{{ field.uid }}</td>
            <td>
                {{ dump(field.settings) }}
            </td>

        </tr>
    {% endfor %}
</table>

<style>
    table {
        width: 100%;
        border-collapse: collapse;
        font-family: sans-serif;
        font-size: 14px;
    }
    table, th, td {
        border: 1px solid;
    }
    td {
        padding: 6px;
    }
</style>